<Window x:Class="GalleryApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GalleryApp"
        mc:Ignorable="d"
        Title="{Binding ElementName=MainFrame, Path=Content.Title}" Height="450" Width="800" MinHeight="650" MinWidth="1200" WindowStartupLocation="CenterScreen">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition/>
            <RowDefinition Height="100"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <!--<Image Source="resources\secondLogo.png" HorizontalAlignment="left" Margin="20,0,0,0"></Image>-->
           <!-- <TextBlock Text="Система для просмотра"    HorizontalAlignment="Center"   VerticalAlignment="Center"    FontSize="30"   FontWeight="Bold"/>-->       
            
        </Grid>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" VerticalAlignment="Center" Grid.Row="1" ></Frame>
        <Grid Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Center">
            <TextBlock FontSize="16">Вы можете позвонить нам по номеру +7 453 233 44 12, если возникли вопросы</TextBlock>
        </Grid>
    </Grid>
</Window>
